Private Sub Form_Load()
Dim RSSchedule As New ADODB.Recordset
Dim RSHitung As New ADODB.Recordset
Dim conn As New ADODB.Connection

Set conn = New ADODB.Connection
    conn.ConnectionString = _
    "Provider=SQLOLEDB.1;Integrated Security=SSPI;Persist Security Info=False;Initial Catalog=simproduksi;Data Source=DESKTOP-KQT6V0C"
    conn.Open

RSSchedule.CursorLocation = adUseClient
RSSchedule.Open " Select id_jadwalproduksi, no_order, waktu_proses, duedate from jadwalproduksi order by waktu_proses asc", conn, 3, 1

With DataGrid1
 Set .DataSource = RSSchedule
 .Refresh
End With

RSHitung.CursorLocation = adUseClient
RSHitung.Open "select flowtime, lateness from hitung", conn, 3, 1

With DataGrid2
 Set .DataSource = RSHitung
 .Refresh
End With

End Sub
